@model IEnumerable<Panchayat.InvItem>

@{
    ViewBag.Title = "Index";
}

<h2>Inventory Items</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Item
        </th>
        <th>
            Register
        </th>
     
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Item)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegisterType.RegisterType1)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ItemID }) 
    </tr>
}

</table>
