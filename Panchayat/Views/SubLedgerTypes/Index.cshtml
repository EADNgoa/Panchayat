@model IEnumerable<Panchayat.SubLedgerType>
@using PagedList.Mvc;

@{
    ViewBag.Title = MyExtensions.CamelToSentence(Panchayat.MyExtensions.GetTitle(GetType().Name));
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().SubLedgerType1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubLedgerType1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SubLedgerTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SubLedgerTypeID }) |
